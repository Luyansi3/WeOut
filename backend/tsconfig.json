{
    
"compilerOptions": {
"outDir": "./dist",
"rootDir": "./src",
"module": "CommonJS",
"strict": true,
"forceConsistentCasingInFileNames": true,
"moduleResolution": "node",
"esModuleInterop": true,
"allowJs": false,
"noEmitOnError": true,
"target": "ES2020",
"lib": ["es2015", "es2020", "dom"]
},
"include": ["./src/**/*.ts"],
"exclude": ["./dist"]
}
